					<a href="../S/S0827.html"><span title="voiceless palato-alveolar sibilant affricate" class="alphaid">tS</span></a> 
					<a href="../S/S0621.html"><span title="voiced bilabial nasal" class="alphaid">m</span></a> 
					<a href="../S/S0635.html"><span title="voiced alveolar nasal" class="alphaid">n</span></a> 
					<a href="../S/S0671.html"><span title="voiced palatal nasal" class="alphaid">nj</span></a> 
					<a href="../S/S0222.html"><span title="voiced velar nasal" class="alphaid">N</span></a> 
					<a href="../S/S0615.html"><span title="voiced alveolar lateral flap" class="alphaid">l[</span></a> 
					<a href="../S/S0568.html"><span title="voiced palatal approximant" class="alphaid">j</span></a> 
					<a href="../S/S0892.html"><span title="voiced labial-velar approximant" class="alphaid">w</span></a> 
					<a href="../S/S0532.html"><span title="high front unrounded vowel" class="alphaid">i</span></a> 
					<a href="../S/S0023.html"><span title="mid front unrounded vowel" class="alphaid">"e</span></a> 
					<a href="../S/S0300.html"><span title="low central unrounded vowel" class="alphaid">a</span></a> 
					<a href="../S/S0066.html"><span title="mid back rounded vowel" class="alphaid">"o</span></a> 
					<a href="../S/S0853.html"><span title="high back rounded vowel" class="alphaid">u</span></a> 
					<a href="../S/S0312.html"><span title="long low central unrounded vowel" class="alphaid">a:</span></a> 
					<a href="../S/S0010.html"><span title="overshort mid central unrounded vowel" class="alphaid">"@S</span></a> 
